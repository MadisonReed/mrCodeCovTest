  },
  "homepage": "https://github.com/MadisonReed/mrCodeCovTest#readme",
  "devDependencies": {
    "codecov": "^3.6.1",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "mocha": "^7.0.0",
    "vue-jest": "^3.0.5"
  }
}
